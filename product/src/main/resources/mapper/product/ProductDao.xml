<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.demo.product.dao.ProductDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.demo.product.entity.ProductEntity" id="productMap">
        <result property="id" column="id"/>
        <result property="originatingStation" column="originating_station"/>
        <result property="stopoverStation" column="stopover_station"/>
        <result property="arrivalStation" column="arrival_station"/>
        <result property="passengerCapacity" column="passenger_capacity"/>
        <result property="startTime" column="start_time"/>
        <result property="trainNumber" column="train_number"/>
        <result property="endTime" column="end_time"/>
        <result property="deleteStatus" column="delete_status"/>
    </resultMap>


</mapper>